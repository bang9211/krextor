<?xml version="1.0" encoding="ISO-8859-1"?>
<scenarios>
    <configuration>
		<!-- 화면에 시나리오 실행내용 출력 여부. (on/off) (default:on) -->
		<define name="display-trace-log" value="on"/>
		<!-- 화면에 시나리오 실행내용 실시간 출력 여부. (on/off) (default:off) -->
		<define name="realtime-display" value="on"/>
		<!-- 시나리오 수행 결과를 파일에 기록할지 여부. (on/off) (default:off) -->
		<define name="file-report" value="off"/>
		<!-- 시나리오 실행 타임아웃 시간(초) (default:0(none)) -->
		<define name="scenario-timeout" value="0"/>
		<!-- 시나리오 수행 후 다음 시나리오 수행 전 유휴 시간(초) (default:0) -->
		<define name="scenario-interval" value="6"/>
		<!-- 시나리오 타입 (seagull/sipp/none) (default:none) -->
		<define name="scenario-type" value="seagull"/>
		<!-- MMCD 연결 정보 - RMI HOST / PORT (default:null) -->
		<!--define name="rmi-host" value="sktdb2"/>
		<define name="rmi-port" value="15003"/-->
		<!-- Report plugin 정보 - library name (default:null) -->
		<!--define name="report-plugin" value="libsimctrl_plugin.so"/-->
		<!-- ignore list - 해당 keyword가 있으면 해당 라인에서 keyword 이후는 비교하지 않는다. -->
		<define name="ignore-keyword" value="|A|PID ["/>
		<define name="ignore-keyword" value="|M| (avp-code = 278)(flags = 64)] = ["/>
		<define name="ignore-keyword" value="|M| (avp-code = 55)(flags = 64)] = ["/>
		<define name="ignore-keyword" value="|M|[HbH-id] = ["/>
		<define name="ignore-keyword" value="|M|[EtE-id] = ["/>
		<define name="ignore-keyword" value="DEV1_1"/>
		<define name="ignore-keyword" value="dev1_1"/>
		<define name="ignore-keyword" value="FreeDosu"/>
		<!-- replace list - 해당 keyword가 있으면 source를 destination으로 변환 후 결과 비교한다. -->
		<!--                src, dst는 value에 선언되며 src + "=>" + dst 형태료 표기해야한다. -->
	</configuration>
	<class name="LTE_DATA_Y">
		<scenario name="data_y_lte" description="LTE 데이터 한도 허용 케이스 (RAT Type = LTE)">
			<command script="$HOME/simulator/seagull/SKTOCS/T2TING_LTE35/data.data_y.sh trace" 
					 check="$HOME/simulator/seagull/SKTOCS/T2TING_LTE35/report/data.data_y.res"/>
		</scenario>
		<scenario name="data_y_3g" description="LTE 데이터 한도 허용 케이스 (RAT Type = 3G)">
			<command script="$HOME/simulator/seagull/SKTOCS/T2TING_LTE35/data.data_y_3g.sh trace" 
			         check="$HOME/simulator/seagull/SKTOCS/T2TING_LTE35/report/data.data_y_3g.res"/>
		</scenario>
		<scenario name="data_y_multi_mscc" description="LTE 데이터 한도 허용 케이스 (MSCC AVP 복수개 설정)">
			<command script="$HOME/simulator/seagull/SKTOCS/T2TING_LTE35/data.data_y_multi_mscc.sh trace" 
			         check="$HOME/simulator/seagull/SKTOCS/T2TING_LTE35/report/data.data_y_multi_mscc.res"/>
		</scenario>
		<scenario name="data_y_multi_session" description="LTE 데이터 한도 허용 케이스 (Session이 복수개 생성)">
			<command script="$HOME/simulator/seagull/SKTOCS/T2TING_LTE35/data.data_y_multi_session.sh trace" 
			         check="$HOME/simulator/seagull/SKTOCS/T2TING_LTE35/report/data.data_y_multi_session.res"/>
		</scenario>
		<scenario name="data_y_not_exist_ccr_init" description="LTE 데이터 한도 허용 케이스 (CCR(Initial-Request) 누락)"> 
			<command script="$HOME/simulator/seagull/SKTOCS/T2TING_LTE35/data.data_y_not_exist_ccr_init.sh trace" 
			         check="$HOME/simulator/seagull/SKTOCS/T2TING_LTE35/report/data.data_y_not_exist_ccr_init.res"/>
		</scenario>
		<scenario name="data_y_not_exist_mscc" description="LTE 데이터 한도 허용 케이스 (MSCC AVP 미설정)"> 
			<command script="$HOME/simulator/seagull/SKTOCS/T2TING_LTE35/data.data_y_not_exist_mscc.sh trace" 
			         check="$HOME/simulator/seagull/SKTOCS/T2TING_LTE35/report/data.data_y_not_exist_mscc.res"/>
		</scenario>
		<scenario name="data_y_null_svc_key" description="LTE 데이터 한도 허용 케이스 (Win-Service-Type AVP 미설정)"> 
			<command script="$HOME/simulator/seagull/SKTOCS/T2TING_LTE35/data.data_y_null_svc_key.sh trace" 
			         check="$HOME/simulator/seagull/SKTOCS/T2TING_LTE35/report/data.data_y_null_svc_key.res"/>
		</scenario>
		<scenario name="data_y_rating_group" description="LTE 데이터 한도 허용 케이스 (Rating-Group AVP에 의해 Quota/VT 설정)">
			<command script="$HOME/simulator/seagull/SKTOCS/T2TING_LTE35/data.data_y_rg.sh trace" 
			         check="$HOME/simulator/seagull/SKTOCS/T2TING_LTE35/report/data.data_y_rg.res"/>
		</scenario>
		<scenario name="data_y_service_id" description="LTE 데이터 한도 허용 케이스 (Service-Identifier AVP에 의해 Q/VT 설정)">
			<command script="$HOME/simulator/seagull/SKTOCS/T2TING_LTE35/data.data_y_sid.sh trace" 
			         check="$HOME/simulator/seagull/SKTOCS/T2TING_LTE35/report/data.data_y_sid.res"/>
		</scenario>
		<scenario name="data_y_usu" description="LTE 데이터 한도 허용 케이스 (Used-Service-Unit 존재)" >
			<command script="$HOME/simulator/seagull/SKTOCS/T2TING_LTE35/data.data_y_usu.sh trace" 
					 check="$HOME/simulator/seagull/SKTOCS/T2TING_LTE35/report/data.data_y_usu.res"/>
		</scenario>
		<scenario name="data_y_too_long" description="LTE 데이터 한도 허용 케이스 (Used-Service-Unit 존재, 15개 존재(메시지가 매우길다.))" >
			<command script="$HOME/simulator/seagull/SKTOCS/T2TING_LTE35/data.data_y_too_long.sh trace"
					 check="$HOME/simulator/seagull/SKTOCS/T2TING_LTE35/report/data.data_y_too_long.res"/>
		</scenario>
	</class>

