	<class name="LTE_DATA_RAR">
		<scenario name="data_y_rar" description="LTE 데이터 한도 허용 -> 소진 전환 케이스">
			<command script="$HOME/simulator/seagull/SKTOCS/T2TING_LTE35/data.data_y_rar.sh trace" 
			         check="$HOME/simulator/seagull/SKTOCS/T2TING_LTE35/report/data.data_y_rar.res"/> 
			<command script="$HOME/simulator/seagull/SKTOCS/T2TING_LTE35/_chg_counter_34_n.sh trace" 
					 check="" display="false" report="false" sleep="2"/>
		</scenario>
		<scenario name="data_n_rar" description="LTE 데이터 한도 소진 -> 허용 전환 케이스">
			<command script="$HOME/simulator/seagull/SKTOCS/T2TING_LTE35/data.data_n_rar.sh trace" 
			         check="$HOME/simulator/seagull/SKTOCS/T2TING_LTE35/report/data.data_n_rar.res"/> 
			<command script="$HOME/simulator/seagull/SKTOCS/T2TING_LTE35/_chg_counter_34_y.sh trace" 
			         check="" display="false" report="false" sleep="2"/>
		</scenario>
	</class>

